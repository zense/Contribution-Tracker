<div class="page-header">
  <%= link_to contributions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_contribution_path(@contribution), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contribution</h1>
</div>

<dl class="dl-horizontal">
  <dt>Type:</dt>
  <dd><%= @contribution.contribution_type %></dd>

  <dt>Name:</dt>
  <dd><%= @contribution.name %></dd>

  <dt>Description:</dt>
  <dd><%= @contribution.description %></dd>

  <dt>Status:</dt>
  <dd><%= @contribution.status %></dd>


</dl>
