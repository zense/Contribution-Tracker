<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>All Members</th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @members) do |member| %>
        <td><%= link_to member.name, "/members/#{member.id}" %> </td>
      <% end %>
    </tbody>
  </table>
</div>
